<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nestedScrollView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#99ffffff"
    android:layoutDirection="ltr"
    >
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"

    android:layout_height="match_parent"
    >
    <android.support.v7.widget.Toolbar xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:local="http://schemas.android.com/apk/res-auto"
        android:id="@+id/app_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layoutDirection="rtl"
        android:background="@color/colorPrimaryDark"
        local:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        local:popupTheme="@style/ThemeOverlay.AppCompat.Light" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/colorText"
            android:text="لوحة التحكم"
            android:layout_gravity="center"
            android:id="@+id/toolbar_title" />

    </android.support.v7.widget.Toolbar>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_centerInParent="true"
        android:layout_margin="20dp"
        android:layout_below="@+id/app_bar"
        >
        <Button
         android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"


            android:background="@drawable/shape2"
            android:text="طلبات المستخدمين "

            android:id="@+id/orders"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:layout_marginTop="30dp"

            android:background="@drawable/shape2"
            android:text="طلبات قيد التنفيذ مع المندوبين "

            android:id="@+id/orders_mandop"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="أضافة قسم"
            android:layout_marginTop="30dp"
            android:id="@+id/add"

            />

        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="أضافة أعلان خارجي"
            android:layout_marginTop="30dp"
            android:id="@+id/addannonce"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="حذف أعلان خارجي"
            android:layout_marginTop="30dp"
            android:id="@+id/deleteannonce"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:visibility="gone"
            android:background="@drawable/shape2"
            android:text="أضافة منتج داخل قسم"
            android:layout_marginTop="30dp"
            android:id="@+id/adddouc"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="حذف قسم"
            android:layout_marginTop="30dp"
            android:id="@+id/delete"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="حذف  قسم فرعي "
            android:layout_marginTop="30dp"
            android:id="@+id/deletedouc"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="حذف اعلان داخلي "
            android:layout_marginTop="30dp"
            android:id="@+id/deleteannn"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text=" تعديل قسم"
            android:layout_marginTop="30dp"
            android:id="@+id/update"

            /> <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textColor="#ffffff"

        android:background="@drawable/shape2"
        android:text="تعديل  قسم فرعي "
        android:layout_marginTop="30dp"
        android:id="@+id/updatedouc"

        />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:visibility="gone"
            android:text="اضافة صورة داخل اعلان"
            android:background="@drawable/shape2"
            android:layout_marginTop="30dp"
            android:id="@+id/noti"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center" android:background="@drawable/shape2"
            android:text="أضافة عرض"
            android:textColor="#ffffff"
            android:visibility="gone"
            android:layout_marginTop="30dp"
            android:id="@+id/offer"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:visibility="gone"
            android:background="@drawable/shape2"
            android:text="مسح عرض"
            android:layout_marginTop="30dp"
            android:id="@+id/offerdelete"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:visibility="gone"
            android:background="@drawable/shape2"
            android:text="أضافة صور عرض"
            android:layout_marginTop="30dp"
            android:id="@+id/offer_img"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="طلبات انضمام التجار"
            android:layout_marginTop="30dp"
            android:id="@+id/tager"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="طلبات انضمام مندوبين"
            android:layout_marginTop="30dp"
            android:id="@+id/mandop"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="تسجيل حساب لتاجر"
            android:layout_marginTop="30dp"
            android:id="@+id/reg_tager"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="مسح طلب تجار"
            android:layout_marginTop="30dp"
            android:id="@+id/del_tager"

            />
        <Button
          android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"
         
            android:background="@drawable/shape2"
            android:text="مسح  تجار"
            android:layout_marginTop="30dp"
            android:id="@+id/deltager"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="مسح  مندوب"
            android:layout_marginTop="30dp"
            android:id="@+id/delmandop"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="انشاء حساب مندوب"
            android:layout_marginTop="30dp"
            android:id="@+id/createmandop"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="كل التجار"
            android:layout_marginTop="30dp"
            android:id="@+id/alltager"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="كل المندوبين"
            android:layout_marginTop="30dp"
            android:id="@+id/allmandop"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="كل المستخدمين"
            android:layout_marginTop="30dp"
            android:id="@+id/allusers"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="كل الاقسام"
            android:layout_marginTop="30dp"
            android:id="@+id/allqesm"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="فاتورة اليوم"
            android:layout_marginTop="30dp"
            android:id="@+id/today"

            />
        <Button
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textColor="#ffffff"

            android:background="@drawable/shape2"
            android:text="فاتورة بالشهر"
            android:layout_marginTop="30dp"
            android:id="@+id/month"

            />
    </LinearLayout>
</RelativeLayout>
</android.support.v4.widget.NestedScrollView>